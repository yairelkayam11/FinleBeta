<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.finalebeta">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ShowFeedback"></activity>
        <activity android:name=".FeedbackAct" />
        <activity android:name=".Creditim" />
        <activity android:name=".OrderDataPreview" />
        <activity android:name=".DinerOrderAct" />
        <activity android:name=".OrderAct" />
        <activity android:name=".CreateEvent" />
        <activity android:name=".AddEventACT" />
        <activity android:name=".SignUpACT" />
        <activity android:name=".SignInACT" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>